{
  "manifest_version": 3,
  "name": "TopDawg",
  "version": "1.0",
  "description": "TopDawg",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {
    "default_title": "TopDawg",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "cookies",
    "storage",
    "unlimitedStorage",
    "tabs",
    "alarms",
    "idle"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "contentScript.js"
      ],
      "css": [
        "contentScript.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/*",
        "fonts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}